{
  "name": "rush-vscode-assistant",
  "version": "0.0.0",
  "description": "VSCode Extension for rush monorepo projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/rushstack.git",
    "directory": "vscode-extensions/rush-vscode-assistant"
  },
  "license": "MIT",
  "author": "William Huang<william2958@gmail.com>",
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "main": "./lib/extension.js",
  "activationEvents": [],
  "scripts": {
    "vscode:prepublish": "npm run build",
    "_phase:build": "heft build --clean",
    "_phase:test": "",
    "build": "heft build --clean",
    "build:watch": "heft build --watch",
    "start": "heft start"
  },
  "contributes": {
    "commands": [
      {
        "command": "rushstack.selectWorkspace",
        "title": "Select workspace"
      }
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "~1.8.6",
    "@rushstack/heft-webpack5-plugin": "workspace:*",
    "@rushstack/heft-typescript-plugin": "workspace:*",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "react-hook-form": "~7.24.1",
    "react-redux": "~8.0.4",
    "redux": "~4.2.0",
    "tslib": "~2.3.1"
  },
  "devDependencies": {
    "@rushstack/eslint-config": "workspace:*",
    "@rushstack/heft": "workspace:*",
    "@rushstack/heft-web-rig": "workspace:*",
    "@rushstack/ts-command-line": "workspace:*",
    "@types/react": "16.14.23",
    "@types/react-dom": "16.9.14",
    "@types/react-redux": "~7.1.22",
    "@types/vscode": "^1.63.0",
    "@types/webpack-env": "1.18.0",
    "eslint": "~8.7.0",
    "html-webpack-plugin": "~5.5.0",
    "typescript": "~5.0.4",
    "webpack": "~5.82.1",
    "webpack-bundle-analyzer": "~4.5.0"
  }
}
